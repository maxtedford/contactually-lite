<li class="contact" data-id="<%= contact.id %>" data-international="<%= contact.international %>" data-extension="<%= contact.extension %>" data-email="<%= contact.email_address %>">
  <h2><%= contact.first_name %> <%= contact.last_name %> (<%= contact.company_name %>)</h2>
  <h4 class="contact-email"><%= contact.email_address %></h4>
  <% if contact.extension != "" && contact.international %>
    <h4>+<%= contact.country_code %> <%= contact.phone_number %> x<%= contact.extension %></h4>
  <% elsif contact.international %>
    <h4>+<%= contact.country_code %> <%= contact.phone_number %></h4>
  <% elsif contact.extension != "" %>
    <h4><%= contact.phone_number %> x<%= contact.extension %></h4>
  <% else %>
    <h4><%= contact.phone_number %></h4>
  <% end %>
  <button href="" class="delete-button">Delete</button>
</li>
