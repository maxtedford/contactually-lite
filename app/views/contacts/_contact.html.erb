<tr class="contact" data-id="<%= contact.id %>" data-international="<%= contact.international %>" data-extension="<%= contact.extension %>" data-email="<%= contact.email_address %>">
  <td><%= contact.first_name %></td>
  <td><%= contact.last_name %></td>
  <td><%= contact.company_name %></td>
  <td class="contact-email"><%= contact.email_address %></td>
  <% if contact.extension != "" && contact.international %>
    <td>+<%= contact.country_code %> <%= contact.phone_number %> x<%= contact.extension %></td>
  <% elsif contact.international %>
    <td>+<%= contact.country_code %> <%= contact.phone_number %></td>
  <% elsif contact.extension != "" %>
    <td><%= contact.phone_number %> x<%= contact.extension %></td>
  <% else %>
    <td><%= contact.phone_number %></td>
  <% end %>
  <td>
    <button href="" class="delete-button btn btn-default">Delete</button>
  </td>
</tr>
