<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h1 id="contacts-title">Your contacts</h1>
    </div>
  </div>
  
  <%= render partial: "filter" %>
  
<table class="table table-bordered table-hover" id="contacts">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Company Name</th>
    <th>Email Address</th>
    <th>Phone Number</th>
    <th></th>
  </tr>
  <%= render partial: "contact", collection: @contacts %>
</table>
</div>
